                console.error('Fehler beim Login:', response.status);\n                setError('Fehler beim Login');\n            }\n\n\n        } catch (err) {\n            console.error('Fehler beim Login:', err);\n            setError('Fehler beim Login');\n        }\n    };\n\n    const showAuth = () => {\n        console.log(token)\n    }\n\n  /*   if (token) {\n        return <Navigate to=\"/\" />;\n    } */\n \n    return (\n        <div>\n            <h2>Login</h2>\n            {error && <div>{error}</div>}\n            <form onSubmit={handleLogin}>\n                <div>\n                    <label>Username:</label>\n                    <input type=\"text\" value={username} onChange={(e) => setUsername(e.target.value)} />\n                </div>\n                <div>\n                    <label>Password:</label>\n                    <input type=\"password\" value={password} onChange={(e) => setPassword(e.target.value)} />\n                </div>\n                <button type=\"submit\">Login</button>\n            </form>\n\n            <button type=\"button\" onClick={showAuth}>Show Auth</button>\n        </div>\n    );\n};\n\nexport default Login;\n","/home/john/Dokumente/projects/taskify/client/src/components/Register.js",["78","79","80"],[],"import axios from 'axios';\nimport React, { useState, useEffect } from 'react';\n\nfunction Register() {\n\n\n    return (\n        <div className=\"main\">\n            <h1>Register</h1>\n            <form>\n                <label>\n                    <p>Username</p>\n                    <input type=\"text\" />\n                </label>\n                <label>\n                    <p>Password</p>\n                    <input type=\"password\" />\n                </label>\n                <div>\n                    <button type=\"submit\">Submit</button>\n                </div>\n            </form>\n        </div>\n    );\n}\n\nexport default Register;\n","/home/john/Dokumente/projects/taskify/client/src/component